        </choice>
      </choices>
    </argument>
    <argument>
      <name>eaves_depth</name>
      <display_name>Eaves Depth</display_name>
      <description>The eaves depth of the roof.</description>
      <type>Double</type>
      <units>ft</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2</default_value>
